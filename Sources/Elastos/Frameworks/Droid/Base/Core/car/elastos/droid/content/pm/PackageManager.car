        InstallStatusToString(
            [in] Int32 status,
            [out] String* str);

        InstallStatusToPublicStatus(
            [in] Int32 status,
            [out] Int32* value);

        /** {@hide} */
        DeleteStatusToString(
            [in] Int32 status,
            [in] String msg,
            [out] String* str);

        /** {@hide} */
        DeleteStatusToString(
            [in] Int32 status,
            [out] String* str);

        /** {@hide} */
        DeleteStatusToPublicStatus(
