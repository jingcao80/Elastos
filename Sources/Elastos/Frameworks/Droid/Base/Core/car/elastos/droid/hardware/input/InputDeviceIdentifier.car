
module
{
    namespace Elastos {
    namespace Droid {
    namespace Hardware {
    namespace Input {

    /**
     * Wrapper for passing identifying information for input devices.
     *
     * @hide
     *
     * @Involve
     * interface IParcelable;
     */
    interface IInputDeviceIdentifier {

        GetDescriptor(
            [out] String* dsp);

        GetVendorId(
            [out] Int32* id);

        GetProductId(
            [out] Int32* id);
    }

    } // namespace Input
    } // namespace Hardware
    } // namespace Droid
    } // namespace Elastos
}
