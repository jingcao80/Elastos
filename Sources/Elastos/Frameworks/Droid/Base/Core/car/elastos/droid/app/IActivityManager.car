        GetConnection(
            [out] IBinder** connection);

        SetConnection(
            [in] IBinder* connection);

        GetNoReleaseNeeded(
            [out] Boolean* noReleaseNeeded);

        SetNoReleaseNeeded(
            [in] Boolean noReleaseNeeded);
    }

