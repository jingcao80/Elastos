    /** Information you can retrieve about a particular application. */
    /**
     * @Involve
     * interface IParcelable
     */
    interface IContentProviderHolder {
        GetProviderInfo(
            [out] IProviderInfo** info);

        SetProviderInfo(
            [in] IProviderInfo* info);

        GetContentProvider(
            [out] IIContentProvider** provider);

        SetContentProvider(
