
module
{

    namespace Elastos {
    namespace Droid {
    namespace Content {
    namespace Pm {

    /** {@hide} */
    [oneway]
    interface IPackageInstallerCallback {
        OnSessionCreated(
            [in] Int32 sessionId);

        OnSessionBadgingChanged(
            [in] Int32 sessionId);

        OnSessionActiveChanged(
            [in] Int32 sessionId,
            [in] Boolean active);

        OnSessionProgressChanged(
            [in] Int32 sessionId,
            [in] Float progress);

        OnSessionFinished(
            [in] Int32 sessionId,
            [in] Boolean success);
    }

    } // namespace Pm
    } // namespace Content
    } // namespace Droid
    } // namespace Elastos
}
