         */
        const String CONTENT_TYPE = "vnd.android.cursor.dir/image";

        /**
         * The default sort order for this table
         */
        const String DEFAULT_SORT_ORDER = "bucket_display_name";  //IMediaStoreImagesImageColumns::BUCKET_DISPLAY_NAME;

        /**
