         */
        GetEXTERNAL_CONTENT_URI(
            [out] IUri** uri);

        Query(
            [in] IContentResolver* cr,
            [in] IUri* uri,
            [in] ArrayOf<String>* projection,
            [out] ICursor** cursor);

        Query(
            [in] IContentResolver* cr,
            [in] IUri* uri,
            [in] ArrayOf<String>* projection,
            [in] String where,
