<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="AlarmManagerDemo"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-permission android:name="android.permission.SET_TIME_ZONE"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name="CActivityOne"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <receiver android:name="CActionBroadCast"> </receiver>
        <service android:name="CActionServiceCast"> </service>
        <activity android:name="CActionActivityCast"> </activity>
    </application>
</manifest>